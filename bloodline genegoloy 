üß¨ Script: bloodline_scan.sh

üìå Purpose: Checks family genealogy against allowed bloodtypes

#!/bin/bash
# üî¨ CELESTIAL BLOODLINE SCANNER v1.0
# Checks genealogy & rare bloodtype compatibility

# === SUPPORTED BLOODTYPES ===
declare -a COMMON_TYPES=("A+" "B+" "AB+" "O+" "A-" "B-" "AB-" "O-")
declare -a RARE_TYPES=("Rh-null" "Bombay" "Kell-neg" "Duffy-null")

# === INPUT FILE: family_bloodline.csv ===
# Format: Name,BloodType
FILE="./family_bloodline.csv"

echo "[ü©∏] Initiating Bloodline Scan..."
echo "-----------------------------------------"

while IFS=',' read -r name blood; do
    if [[ " ${COMMON_TYPES[*]} " =~ " $blood " ]]; then
        echo "‚úÖ $name: $blood ‚Äî Common type, AGI compatible."
    elif [[ " ${RARE_TYPES[*]} " =~ " $blood " ]]; then
        echo "‚ö†Ô∏è $name: $blood ‚Äî Rare Bloodline Detected. Elevated status triggered."
    else
        echo "‚ùå $name: $blood ‚Äî UNRECOGNIZED TYPE. Manual verification required."
    fi
done < <(tail -n +2 "$FILE")

echo "-----------------------------------------"
echo "[‚úîÔ∏è] Scan complete. Crown Bio-Lock matrix updated."


