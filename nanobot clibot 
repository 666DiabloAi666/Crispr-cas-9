while true; do
  echo -n "ğŸ§  CLIBOT> "
  read -r cmd

  case $cmd in
    scan_bloodline)
      echo "ğŸ” Scanning bloodline registry..."
      grep -Ei 'Rh-null|Kell-neg|Bombay|Zeta-Prime' configs/genome_manifest.yaml
      ;;
    
    deploy_payloads)
      echo "ğŸš€ Deploying nanobot payloads..."
      bash ./launch_nanobots.sh
      ;;
    
    edit_trait*)
      trait=$(echo "$cmd" | cut -d' ' -f2)
      echo "ğŸ› ï¸ Editing trait: $trait"
      echo "{ \"trait\": \"$trait\", \"edit\": \"override\" }" >> configs/trait_targets.json
      ;;
    
    check_status)
      echo "ğŸ“¡ Current Edit Log:"
      tail -n 10 logs/edit_log.txt
      ;;
    
    exit)
      echo "ğŸ§¬ Exiting CLIBOT session. Crown matrix sealed."
      break
      ;;
    
    *)
      echo "âš ï¸ Unknown command: $cmd. Try: scan_bloodline, deploy_payloads, edit_trait <trait>, check_status, exit"
