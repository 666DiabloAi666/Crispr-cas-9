echo "[âœ”ï¸] All edits processed. Genome sealed."


---

ğŸ§  scripts/render_yaml_to_json.py

Python: Converts YAML Manifest to JSON

import yaml, json

with open("genome_manifest.yaml", "r") as f:
    data = yaml.safe_load(f)

with open("genome_manifest.json", "w") as f:
    json.dump(data, f, indent=2)

print("[ğŸ“˜] YAML manifest converted to JSON.")


